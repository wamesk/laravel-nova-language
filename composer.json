{
    "name": "wamesk/laravel-nova-language",
    "description": "Laravel Nova 4 Language field.",
    "version": "4.0.0",
    "keywords": [
        "laravel",
        "nova",
        "nova4",
        "language",
        "field"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "WAME s.r.o.",
            "homepage": "https://wame.sk"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "laravel/nova": "^4.0",
        "rinvex/languages": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Wame\\LaravelNovaLanguage\\": "src/",
            "Wame\\LaravelNovaLanguage\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/Utils/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wame\\LaravelNovaLanguage\\Providers\\PackageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
