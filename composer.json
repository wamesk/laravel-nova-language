{
    "name": "wamesk/laravel-nova-language",
    "description": "Laravel Nova 4 Language field.",
    "keywords": [
        "laravel",
        "nova",
        "nova4",
        "language",
        "field"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "WAME s.r.o.",
            "homepage": "https://wame.sk"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "laravel/nova": "^4.0",
        "rinvex/languages": "^7.1",
        "wamesk/laravel-commands": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Wame\\LaravelNovaLanguage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wame\\LaravelNovaLanguage\\PackageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
