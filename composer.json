{
    "name": "wamesk/laravel-nova-language",
    "description": "Laravel Nova Language field.",
    "version": "5.0.1",
    "keywords": [
        "laravel",
        "nova",
        "nova5",
        "language",
        "field"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "WAME s.r.o.",
            "homepage": "https://wame.sk"
        }
    ],
    "require": {
        "laravel/nova": "^5.0",
        "rinvex/languages": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Wame\\LaravelNovaLanguage\\": "src/",
            "Wame\\LaravelNovaLanguage\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/Utils/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wame\\LaravelNovaLanguage\\Providers\\LaraveNovaLanguageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
